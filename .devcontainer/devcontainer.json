{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"features": {
		"ghcr.io/devcontainers-contrib/features/pipenv:2": {
			"version": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools",
				"ms-python.python"
			]
		}
	},
	"postCreateCommand": "bash .devcontainer/postCreateCommand.sh"
}